{
  "headers": [
    {
      "source": "/static/(.*)", 
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store"
        }
      ],
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],

  "rewrites": [{ "source": "/(.*)", "destination": "index.js" }]
}
